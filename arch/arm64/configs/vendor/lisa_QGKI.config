CONFIG_BOARD_XIAOMI_LISA=y

# Fingerprint
CONFIG_FINGERPRINT_FPC_TEE=m

# Haptics
# CONFIG_INPUT_QCOM_HV_HAPTICS is not set
CONFIG_INPUT_AW8624_HAPTIC=y

# Pinctrl
CONFIG_PINCTRL_SM7325=y

# Proximity
CONFIG_MIUS_PROXIMITY=y

# Touchscreen
CONFIG_TOUCHSCREEN_GOODIX_BRL=m
CONFIG_TOUCHSCREEN_GOODIX_TS_GESTURE=m
CONFIG_TOUCHSCREEN_GOODIX_TS_TOOLS=m
CONFIG_TOUCHSCREEN_QGKI_GOODIX=y
CONFIG_TOUCHSCREEN_XIAOMI_TOUCHFEATURE=m

# WiFi
CONFIG_CNSS_QCA6750=y
