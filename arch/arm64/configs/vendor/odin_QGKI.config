CONFIG_BOARD_XIAOMI_ODIN=y

# Fingerprint
CONFIG_FINGERPRINT_GOODIX_FOD=m

# Haptics
# CONFIG_INPUT_QCOM_HV_HAPTICS is not set
CONFIG_INPUT_AW8697_HAPTIC=y

# Power supply
CONFIG_BQ_FUEL_GAUGE=y

# Proximity
CONFIG_MIUS_PROXIMITY=y

# Time of flight
CONFIG_STMVL53L5=m

# Touchscreen
CONFIG_TOUCHSCREEN_CYPRESS_CYTTSP5=m
CONFIG_TOUCHSCREEN_CYPRESS_CYTTSP5_DEVICE_ACCESS=m
CONFIG_TOUCHSCREEN_CYPRESS_CYTTSP5_I2C=m
CONFIG_TOUCHSCREEN_CYPRESS_CYTTSP5_LOADER=m
CONFIG_TOUCHSCREEN_ST_FTS_V521_SPI=m
CONFIG_TOUCHSCREEN_XIAOMI_TOUCHFEATURE=m

# Ultra wideband
CONFIG_NXP_SR100=m

# WiFi
CONFIG_CNSS_QCA6490=y

# Wireless charging
CONFIG_MI_WIRELESS=y
