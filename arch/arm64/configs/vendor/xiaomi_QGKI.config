# CPU Mask
CONFIG_LITTLE_CPU_MASK=15
CONFIG_BIG_CPU_MASK=112
CONFIG_PRIME_CPU_MASK=128

# Coresight
# CONFIG_CORESIGHT is not set
# CONFIG_CORESIGHT_QGKI is not set
# CONFIG_CORESIGHT_LINK_AND_SINK_TMC is not set
# CONFIG_CORESIGHT_CTI is not set
# CONFIG_CORESIGHT_CTI_SAVE_DISABLE is not set
# CONFIG_CORESIGHT_TPDA is not set
# CONFIG_CORESIGHT_TPDM is not set
# CONFIG_CORESIGHT_HWEVENT is not set
# CONFIG_CORESIGHT_DUMMY is not set
# CONFIG_CORESIGHT_REMOTE_ETM is not set
# CONFIG_CORESIGHT_TGU is not set

# Device tree
CONFIG_BUILD_ARM64_DT_OVERLAY=y

# DAMON
CONFIG_DAMON=y
CONFIG_DAMON_VADDR=y
CONFIG_DAMON_PADDR=y
CONFIG_DAMON_SYSFS=y
CONFIG_DAMON_RECLAIM=y
CONFIG_DAMON_LRU_SORT=y

# File systems
CONFIG_EXFAT_FS=y
CONFIG_EROFS_FS_PCPU_KTHREAD=y
CONFIG_EROFS_FS_PCPU_KTHREAD_HIPRI=y

# Fingerprint
CONFIG_INPUT_FINGERPRINT=y

# Hardware ID
CONFIG_MI_HARDWARE_ID=m

# GPU
CONFIG_QCOM_KGSL_IDLE_TIMEOUT=58

# IPC
# CONFIG_IPC_LOGGING is not set

# IR
CONFIG_IR_SPI=m
CONFIG_LIRC=y
CONFIG_RC_CORE=y
CONFIG_RC_DECODERS=y
CONFIG_RC_DEVICES=y

# Power supply
# CONFIG_BACKLIGHT_QCOM_SPMI_WLED is not set
CONFIG_BQ2597X=y
CONFIG_LEDS_QTI_FLASH=m
CONFIG_QTI_BATTERY_CHARGER=m

# RCU Lazy
CONFIG_RCU_LAZY=y

# SCHED
CONFIG_CPU_IDLE_GOV_TEO=y
# CONFIG_MQ_IOSCHED_DEADLINE is not set
# CONFIG_MQ_IOSCHED_KYBER is not set
# CONFIG_IOSCHED_BFQ=y is not set
# CONFIG_BFQ_GROUP_IOSCHED is not set
CONFIG_MQ_IOSCHED_SSG=y
CONFIG_MQ_IOSCHED_SSG_CGROUP=y

# Thermal
CONFIG_MI_THERMAL_INTERFACE=m

# Touchscreen
# CONFIG_TOUCHSCREEN_FTS is not set
# CONFIG_TOUCHSCREEN_NT36XXX is not set
# CONFIG_TOUCHSCREEN_ST is not set

# WiFi
CONFIG_ICNSS2=m
# CONFIG_ICNSS2_RESTART_LEVEL_NOTIF is not set
CONFIG_CNSS2=m
CONFIG_QCA_CLD_WLAN=m
