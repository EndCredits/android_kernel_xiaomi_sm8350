CONFIG_BOARD_XIAOMI=y

# Versioning
CONFIG_LOCALVERSION="-tailwind-v1.3"
# CONFIG_LOCALVERSION_AUTO is not set

# Arch specific
# CONFIG_ARM64_SW_TTBR0_PAN is not set
# CONFIG_EDAC is not set
# CONFIG_EDAC_QGKI is not set
# CONFIG_EDAC_KRYO_ARM64 is not set
# CONFIG_EDAC_KRYO_ARM64_PANIC_ON_UE is not set
# CONFIG_EDAC_QCOM is not set
# CONFIG_EDAC_QCOM_LLCC_PANIC_ON_UE is not set

# CPU Freq
CONFIG_CPU_FREQ_DEFAULT_GOV_SCHEDUTIL=y
CONFIG_UCLAMP_BUCKETS_COUNT=20

# Device tree
CONFIG_BUILD_ARM64_DT_OVERLAY=y

# Debug FS
# CONFIG_DEBUG_FS is not set
# CONFIG_STACKTRACE_SUPPORT is not set
# CONFIG_PAGE_OWNER is not set
# CONFIG_DEBUG_KERNEL is not set

# DAMON
CONFIG_DAMON=y
CONFIG_DAMON_VADDR=y
CONFIG_DAMON_PADDR=y
CONFIG_DAMON_SYSFS=y
CONFIG_DAMON_RECLAIM=y
CONFIG_DAMON_LRU_SORT=y

# File systems
CONFIG_EXFAT_FS=y
CONFIG_EROFS_FS_PCPU_KTHREAD=y
CONFIG_EROFS_FS_PCPU_KTHREAD_HIPRI=y
CONFIG_FS_HPB=y

# Fingerprint
CONFIG_INPUT_FINGERPRINT=y

# Filesystem
CONFIG_F2FS_UNFAIR_RWSEM=y

# Hardware ID
CONFIG_MI_HARDWARE_ID=m

# IPC
# CONFIG_USB_F_FS_IPC_LOGGING is not set
# CONFIG_IPC_LOGGING is not set
CONFIG_ENABLE_DEFAULT_TRACERS is not set

# IR
CONFIG_IR_SPI=m
CONFIG_LIRC=y
CONFIG_RC_CORE=y
CONFIG_RC_DECODERS=y
CONFIG_RC_DEVICES=y

# Memory Management
# CONFIG_MSM_GPI_DMA_DEBUG is not set
# CONFIG_MSM_DEBUGCC_LAHAINA is not set
# CONFIG_SM_DEBUGCC_SHIMA is not set
# CONFIG_MULTIPLE_KSWAPD is not set
CONFIG_USERFAULTFD=y

# NVDIMM
# CONFIG_LIBNVDIMM is not set
# CONFIG_DAX is not set

# Power supply
# CONFIG_BACKLIGHT_QCOM_SPMI_WLED is not set
CONFIG_BQ2597X=y
CONFIG_LEDS_QTI_FLASH=m
CONFIG_QTI_BATTERY_CHARGER=m

# RCU Lazy
CONFIG_RCU_LAZY=y

# RMNET
CONFIG_RMNET_OFFLOAD=m
CONFIG_RMNET_SHS=m
CONFIG_RMNET_CORE=m
CONFIG_RMNET_CTL=m

# SCHED
CONFIG_CPU_IDLE_GOV_TEO=y
# CONFIG_HZ_250 is not set
CONFIG_HZ_100=y
CONFIG_SUSPEND_SKIP_SYNC=y
# CONFIG_CPU_FREQ_TIMES is not set
# CONFIG_SCHED_DEBUG is not set
# CONFIG_SCHEDSTATS is not set

# Spectre v2
# CONFIG_HARDEN_BRANCH_PREDICTOR is not set

# Stack size warning
CONFIG_FRAME_WARN=0

# Thermal
CONFIG_MI_THERMAL_INTERFACE=m

# Touchscreen
# CONFIG_TOUCHSCREEN_FTS is not set
# CONFIG_TOUCHSCREEN_NT36XXX is not set
# CONFIG_TOUCHSCREEN_ST is not set

# UFS
CONFIG_UFSFEATURE=y
CONFIG_UFSHPB=y
CONFIG_UFSTW=y
CONFIG_OPLUS_FEATURE_UFS=y

# WiFi
CONFIG_ICNSS2=m
# CONFIG_ICNSS2_RESTART_LEVEL_NOTIF is not set
CONFIG_CNSS2=m
CONFIG_QCA_CLD_WLAN=m
